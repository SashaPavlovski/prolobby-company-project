<?xml version="1.0" encoding="utf-8" ?>
<!-- XSD manual extracted from package NLog.Schema: https://www.nuget.org/packages/NLog.Schema-->
<nlog 
      autoReload="true"
	  internalLogToConsoleError="true"
      internalLogLevel="Info"
	  internalLogIncludeTimestamp="true"
	  throwConfigExceptions="true">

	<!-- the targets to write to -->
	<targets>
		<!-- write logs to file -->
		<target type="File" name="logfile" fileName="C:\Users\User\source\repos\azur-function-class\ProLobbyCompanyProject\ProLobbyCompanyProject-server\logFile\${date:format=yyyy-MM-dd_HH}-log.txt"
				layout="${longdate}|${level}|${Logger}|${message}|${all-event-properties} ${exception:format=tostring}" />
		
	</targets>

	<!-- rules to map from logger name to target -->
	<rules>
		<logger name="*" minlevel="Trace" writeTo="logfile" />
	</rules>
</nlog>
